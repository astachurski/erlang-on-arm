#ifndef BEAM_EMU_H_
#define BEAM_EMU_H_

#define CALL_EXT 0x07
#define CALL_EXT_LAST 0x08
#define BIF0 0x09
#define BIF1 0x0A
#define BIF2 0x0B

#define CALL_EXT_ONLY 0x4E

#define GC_BIF1 7C
#define GC_BIF2 7D
#define GC_BIF3 98

#endif /* BEAM_EMU_H_ */
